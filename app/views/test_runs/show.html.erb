<% if @include_audit_nav %>
  <%= render 'audits/show_page_header' %>
<% else %>
  <h1 class='page-title with-sub-title'>Test Run Result</h1>
  <h2 class='page-sub-title'><%= @functional_test.title %></h2>

  <%= render 'functional_tests/nav' %>
<% end %>

<div class='tagsafe-container col-10 offset-1'>
  <%= render 'show', functional_test: @functional_test, test_run: @test_run, back_link: @back_link %>
</div>