<div class="row">
  <div class="col-6">
    <div class="tag-safe-container">
      <h5 class="text-center audit-type-title">Results With <%= @script_subscriber.try_friendly_name %></h5>
      <div class="log-container">
        <%= @with_tag_log.logs.html_safe %>
      </div>
    </div>
  </div>
  <div class="col-6">
    <div class="tag-safe-container">
      <h5 class="text-center audit-type-title">Results Without <%= @script_subscriber.try_friendly_name %></h5>
      <div class="log-container">
        <%= @without_tag_log.logs.html_safe %>
      </div>
    </div>
  </div>
</div>

<script>
  $('[data-request-url]').hover(function(e) {
    var url = e.target.getAttribute('data-request-url');
    $('[data-request-url="'+url+'"]').addClass('hovered');
  })
  $('[data-request-url]').mouseout(function(e) {
    var url = e.target.getAttribute('data-request-url');
    $('[data-request-url="'+url+'"]').removeClass('hovered');
  });
</script>