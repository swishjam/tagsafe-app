<%= turbo_frame_tag "#{container.uid}_container_tags_table", class: 'mt-6 flow-root' do %>
  <% if tag_snippets.any? || local_assigns[:allow_empty_table] %>

    <ul id="<%= "#{container.uid}_container_tags_table_rows" %>" is="turbo-frame" role="list" class="-my-5 divide-y divide-gray-200">
      <% tag_snippets.each do |tag_snippet| %>
        <%= render 'server_loadable_partials/tags/tag_table_row', tag_snippet: tag_snippet, container: container %>
      <% end %>
    </ul>

  <% end %>
<% end %>
