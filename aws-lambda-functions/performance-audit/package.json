{
  "name": "performance-audit",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "deploy-prod": "serverless deploy --stage production",
    "deploy-staging": "serverless deploy --stage staging",
    "deploy-dev": "serverless deploy --stage collin-dev",
    "locally": "serverless invoke local --function run-performance-audit --path ../test-data/performance-audit/test.json --stage local",
    "domain-audit-locally": "serverless invoke local --function run-performance-audit --path ../test-data/performance-audit/domain-audit-test.json --stage local",
    "local-generate-cache": "serverless invoke local --function generate-cache --path ../test-data/performance-audit/test.json --stage local",
    "tail-dev-logs": "serverless logs --function run-performance-audit --stage collin-dev -t",
    "tail-prod-logs": "serverless logs --function run-performance-audit --stage production -t"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "aws-sdk": "^2.1048.0",
    "jest": "^27.5.1",
    "puppeteer": "^10.1.0",
    "serverless": "^2.71.0",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-datadog": "^5.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "chrome-aws-lambda": "^10.1.0",
    "dotenv": "^10.0.0",
    "lighthouse": "^9.5.0",
    "node-fetch": "^2.0.0",
    "puppeteer-core": "^10.4.0",
    "puppeteer-screen-recorder": "^2.0.2",
    "speedline-core": "^1.4.3"
  }
